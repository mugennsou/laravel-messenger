{
    "name": "mugennsou/laravel-messenger",
    "description": "A Overtrue/EasySms bridge for Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "aki",
            "email": "mj501738931@126.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/notifications": "^5.5",
        "illuminate/support": "^5.5",
        "overtrue/easy-sms": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Mugennsou\\LaravelMessenger\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mugennsou\\LaravelMessenger\\MessengerServiceProvider"
            ],
            "aliases": {
                "Messenger": "Mugennsou\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    }
}